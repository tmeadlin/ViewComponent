@model IList<int>

<table class="table">
    <thead>
        <tr>
            <th>Product Info</th>
            <th>Price</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var id in Model)
        {
            @*<tr>
                <td>
                    <span>here: @id</span>
                </td>
            </tr>*@
            
            @await Component.InvokeAsync("ProductItem", new { productId = id })
        }
    </tbody>
</table>